{
  "name": "Lastly",
  "version": "1.0.1",
  "description": "Quickly open and manage your recently added bookmarks",
  "permissions": [
    "bookmarks",
    "tabs"
  ],
  "browser_action": {
      "default_title": "Lastly",
      "default_icon": "icon.png",
      "default_popup": "lastly.html"
  },
  "icons": { "16": "lastmark-16.png",
           "48": "lastmark-48.png",
          "128": "lastmark-128.png" },

  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
